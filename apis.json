{
  "settings": {
    "strict_mode": true
  },
  "apis": [
    {
      "paths":         ["/info", "/status"],
      "methods":       ["GET"],
      "accept":        ["all"],
      "mock_response": {
        "result": {
          "name": "magic-mock-api",
          "version": ".0.9.5",
          "description": "mock rest api tool"
        },
        "status_code": 200
      }
    },
    {
      "paths":         ["/register"],
      "methods":       ["POST", "PUT"],
      "accept":        ["application/json", "application/x-www-form-urlencoded"],
      "mock_response": {
        "result": {
          "act_type": 1,
          "status": "success",
          "register_id": "a99f391852caf97496e7a5ad27a7f295ecc194061b490985959472f3da3d00fb"
        },
        "status_code": 200
      }
    },
    {
      "paths":         ["/heartbeat"],
      "methods":       ["POST"],
      "accept":        ["application/json", "application/x-www-form-urlencoded", "text/html", "application/octet-stream", "text/plain"],
      "mock_response": {
        "result": {
          "act_type": 1,
          "status": "success",
          "message": "OK, keep in touch"
        },
        "status_code": 200
      }
    },
    {
      "paths":         ["/notify"],
      "methods":       ["POST"],
      "accept":        ["all"],
      "mock_response": {
        "result": {
          "act_type": 1,
          "status": "acknowledged"
        },
        "status_code": 200
      }
    },
    {
      "paths":         ["/foo"],
      "methods":       ["POST"],
      "accept":        ["application/json"],
      "mock_response": {
        "result": {
          "bar": "okay",
          "message": "test accept type"
        },
        "status_code": 200
      }
    }
  ]
}
